<footer class="text-center">
    <div class="container">
      <div class="row">
        <div class="col-lg-8 col-md-10 mx-auto">
			<% if (config.github_username) { %>
			  <a class="text-muted badge badge-light" target="_blank" rel="noopener" href="https://github.com/<%= config.github_username %>">Github</a>
			<% } %>
			<% if (config.juejin_userid) { %>
			  <a class="text-muted badge badge-light" target="_blank" rel="noopener" href="https://juejin.im/user/<%= config.juejin_userid %>">掘金</a>
			<% } %>
        	<p class="copyright text-muted">Copyright © <a href="/">coldstone.fun</a> 2019</p>
        </div>
      </div>
	</div>
</footer>
